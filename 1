from django.contrib import admin
from django.urls import path
from app.views import home_view, current_time_view, workdir_view  # Импортируем новые view-функции

urlpatterns = [
    path('', home_view, name='home'),
    path('current_time/', current_time_view, name='current_time'),  # Добавляем URL для current_time_view
    path('workdir/', workdir_view, name='workdir'),  # Добавляем URL для workdir_view
    path('admin/', admin.site.urls),
]
